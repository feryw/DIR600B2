.S.s:
	$(CPP) $(CFLAGS) $< -o $*.s
.S.o:
	$(CC) $(CFLAGS) -c $< -o $*.o

obj-y    := mipsIRQ.o reset.o init.o irq.o \
            memory.o printf.o cmdline.o setup.o time.o 
obj-$(CONFIG_PCI) += pci.o
obj-$(CONFIG_DWC_OTG) += lm.o

ifdef CONFIG_RALINK_TIMER
obj-$(CONFIG_RALINK_TIMER) +=  rt_timer.o
endif

